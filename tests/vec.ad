fn main() {
    let mut a: Vec<i32> = Vec::new();
    a.push(1);
    a.push(2);
    assert_eq!(1, a[0]);
    assert_eq!(2, a[1]);

    let mut a = Vec::new();
    a.push('a');
    a.push('b');
    assert_eq!('a', a[0]);
    assert_eq!('b', a[1]);

    let abc = "abc".chars();
    assert_eq!('a', abc[0]);
    assert_eq!('b', abc[1]);
    assert_eq!('c', abc[2]);

    println!("ok");
}
